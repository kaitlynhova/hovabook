<h1>New session</h1>

    <%= form_tag sessions_path do %>
    <p>
      <%= text_field_tag :email,nil, :placeholder => "EMAIL", :class => "inputtimes"%>
    </p>
    <p>
      <%= password_field_tag :password,nil, :placeholder => "PASSWORD", :class => "inputtimes" %>
    </p>
    <p>
      <%= submit_tag "Log In", :class => "submit_login" %>
    </p>
    <% end %>

<%= link_to 'Back', sessions_path %>
